    <DataSet Name="DataSet4">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT s.st_id,s.fname+' '+s.lname AS FullName ,sc.grade,c.courses_id,c.course_name
FROM Student s INNER JOIN Student_course sc
ON s.st_id = sc.st_id INNER JOIN Course c ON sc.course_id = c.courses_id
</CommandText>
      </Query>
      <Fields>
        <Field Name="st_id">
          <DataField>st_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FullName">
