  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT s.st_id,s.fname,s.lname,s.st_gender,s.st_phone,s.st_birthdate,s.st_joindate,s.street , d.dept_name
FROM Student s INNER JOIN Department d
on s.dept_id =  d.dept_id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
