          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="course_name">
          <DataField>course_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet5">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT COUNT(sc.st_id) AS Student_Number, i.fname+' '+i.lname AS
FullName , c.course_name
FROM Instructor i INNER JOIN Instructor_courses ic
ON i.insrt_id = ic.instr_id INNER JOIN Course c 
ON ic.course_id = c.courses_id INNER JOIN Student_course sc
ON c.courses_id = sc.course_id
GROUP BY i.fname,i.lname  , c.course_name</CommandText>
      </Query>
      <Fields>
        <Field Name="Student_Number">
          <DataField>Student_Number</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="course_name">
          <DataField>course_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
